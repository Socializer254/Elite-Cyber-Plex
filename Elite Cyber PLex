<span class="" data-state="closed"><button class="rounded-lg text-token-text-secondary hover:bg-token-main-surface-secondary" aria-label="Edit in canvas"><span class="flex h-[30px] w-[30px] items-center justify-center"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-md"><path d="M2.5 5.5C4.3 5.2 5.2 4 5.5 2.5C5.8 4 6.7 5.2 8.5 5.5C6.7 5.8 5.8 7 5.5 8.5C5.2 7 4.3 5.8 2.5 5.5Z" fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"></path><path d="M5.66282 16.5231L5.18413 19.3952C5.12203 19.7678 5.09098 19.9541 5.14876 20.0888C5.19933 20.2067 5.29328 20.3007 5.41118 20.3512C5.54589 20.409 5.73218 20.378 6.10476 20.3159L8.97693 19.8372C9.72813 19.712 10.1037 19.6494 10.4542 19.521C10.7652 19.407 11.0608 19.2549 11.3343 19.068C11.6425 18.8575 11.9118 18.5882 12.4503 18.0497L20 10.5C21.3807 9.11929 21.3807 6.88071 20 5.5C18.6193 4.11929 16.3807 4.11929 15 5.5L7.45026 13.0497C6.91175 13.5882 6.6425 13.8575 6.43197 14.1657C6.24513 14.4392 6.09299 14.7348 5.97903 15.0458C5.85062 15.3963 5.78802 15.7719 5.66282 16.5231Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path><path d="M14.5 7L18.5 11" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path></svg></span></button></span></div></div></div></div></div></div></div></div></article><article class="group/turn w-full text-token-text-primary focus-visible:outline-2 focus-visible:outline-offset-[-4px]" dir="auto" data-testid="conversation-turn-20" data-scroll-anchor="false" style="scroll-margin-top: -3704px;"><h5 class="sr-only">You said:</h5><div class="text-base my-auto mx-auto py-[18px] px-6"><div class="mx-auto flex flex-1 text-base gap-4 md:gap-5 lg:gap-6 md:max-w-3xl"><div class="group/conversation-turn relative flex w-full min-w-0 flex-col @xs/thread:px-0 @sm/thread:px-1.5 @md/thread:px-4"><div class="flex-col gap-1 md:gap-3"><div class="flex max-w-full flex-col flex-grow"><div data-message-author-role="user" data-message-id="26840d68-431c-44df-9381-17896032363f" dir="auto" class="min-h-8 text-message relative flex w-full flex-col items-end gap-2 whitespace-normal break-words text-start [.text-message+&amp;]:mt-5"><div class="w-full"><div class="flex w-full flex-col gap-1 empty:hidden items-end rtl:items-start"><div class="relative max-w-[var(--user-chat-width,70%)] rounded-3xl bg-token-message-surface px-5 py-2.5"><div class="whitespace-pre-wrap">from flask import Flask, request, jsonify, render_template
import os
import requests
import json
import numpy as np
from sklearn.linear_model import LogisticRegression
import logging

# Initialize Flask app
app = Flask(__name__, static_url_path='', static_folder='static', template_folder='templates')

# Configure Logging
logging.basicConfig(level=logging.INFO, format='%(asctime)s - %(levelname)s - %(message)s')

# Load API Key, Webhook URL, and Port from environment variables
API_KEY = os.getenv("BOT_API_KEY", "your_default_api_key")
BOT_WEBHOOK_URL = os.getenv("BOT_WEBHOOK_URL", "https://your-bot-webhook.com")
DERIV_API_URL = os.getenv("DERIV_API_URL", "https://api.deriv.com/v2/trading")
PORT = int(os.getenv("PORT", 5000))
RENDER_HOST = os.getenv("RENDER_HOST", "0.0.0.0")
ROOT_PATH = os.getenv("ROOT_PATH", "/")

# Ensure all required environment variables are set
REQUIRED_ENV_VARS = ["BOT_API_KEY", "BOT_WEBHOOK_URL", "DERIV_API_URL"]
for var in REQUIRED_ENV_VARS:
    if not os.getenv(var):
        logging.error(f"Missing required environment variable: {var}")
        raise EnvironmentError(f"Missing required environment variable: {var}")

# List of Volatility Indices to analyze
VOLATILITY_INDICES = [
    "R_10", "R_25", "R_50", "R_75", "R_100", "R_150", "R_200", "R_300", "R_500", "1s"
]

# Historical Data Storage
historical_data = {index: [] for index in VOLATILITY_INDICES}
models = {index: LogisticRegression() for index in VOLATILITY_INDICES}

def analyze_digit_match(data, index):
    """Analyzes digit matches and returns a trade signal using AI model."""
    probability = data.get("probability", 0)
    digit = data.get("digit", "?")
    
    if len(historical_data[index]) &gt; 10:
        try:
            X = np.array([d["features"] for d in historical_data[index]])
            y = np.array([d["label"] for d in historical_data[index]])
            models[index].fit(X, y)
            prediction = models[index].predict([[probability]])[0]
            action = "BUY" if prediction == 1 else "HOLD"
        except Exception as e:
            logging.error(f"Model training error: {e}")
            action = "HOLD"
    else:
        action = "BUY" if probability &gt; 0.85 else "HOLD"
    
    return {"action": action, "digit": digit, "confidence": probability, "index": index}

def fetch_market_data(index):
    """Fetches real-time market data from Deriv API for a given index."""
    try:
        response = requests.get(f"{DERIV_API_URL}/{index}")
        response.raise_for_status()
        return response.json()
    except requests.exceptions.RequestException as e:
        logging.error(f"Error fetching market data: {e}")
        return {"error": f"Failed to fetch market data for {index}"}

def send_signal_to_bot(signal_data):
    """Sends trading signal to the bot webhook endpoint."""
    headers = {"Content-Type": "application/json"}
    try:
        response = requests.post(BOT_WEBHOOK_URL, json=signal_data, headers=headers)
        response.raise_for_status()
        return response.json()
    except requests.exceptions.RequestException as e:
        logging.error(f"Error sending signal to bot: {e}")
        return {"error": "Failed to send signal"}

@app.route("/api/signal", methods=["POST"])
def receive_signal():
    """Receives a signal, analyzes it, executes trade, and sends signal to bot."""
    if request.headers.get("API_KEY") != API_KEY:
        return jsonify({"error": "Unauthorized"}), 403
    
    data = request.json
    index = data.get("index")
    
    if index not in VOLATILITY_INDICES:
        return jsonify({"error": "Invalid index"}), 400
    
    result = analyze_digit_match(data, index)
    
    if result["action"] == "BUY":
        execute_trade(result["digit"], index)
        send_signal_to_bot(result)
    
    return jsonify(result)

def execute_trade(digit, index):
    """Automates contract execution on Deriv API for a given index."""
    trade_payload = {
        "contract_type": "DIGITMATCH",
        "symbol": index,
        "stake": 10,
        "duration": 1,
        "duration_unit": "t",
        "barrier": digit
    }
    try:
        response = requests.post(DERIV_API_URL, json=trade_payload)
        response.raise_for_status()
        return response.json()
    except requests.exceptions.RequestException as e:
        logging.error(f"Error executing trade: {e}")
        return {"error": "Trade execution failed"}

@app.route("/api/market", methods=["GET"])
def get_market_data():
    """Endpoint to fetch real-time market data for all indices."""
    market_data = {index: fetch_market_data(index) for index in VOLATILITY_INDICES}
    return jsonify(market_data)

@app.route("/", methods=["GET", "POST"])
def login():
    if request.method == "POST":
        username = request.form.get("username")
        password = request.form.get("password")
        if username == "socializer" and password == "Jw@socializer4":
            return "Elite Cyber Tech API is Running!"
        else:
            return "Invalid Credentials", 403
    return render_template("login.html")

if __name__ == "__main__":
    from waitress import serve
    logging.info("Starting Elite Cyber Tech API...")
    serve(app, host=RENDER_HOST, port=PORT)
